{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-showcase1"
		},
		"sqldbshowcase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlshowcase.database.windows.net"
		},
		"sqldbshowcase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldb-showcase"
		},
		"stshowcase1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stshowcase1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-showcase1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sqldbshowcase_properties_typeProperties_server": {
						"value": "[parameters('sqldbshowcase_properties_typeProperties_server')]"
					},
					"sqldbshowcase_properties_typeProperties_database": {
						"value": "[parameters('sqldbshowcase_properties_typeProperties_database')]"
					},
					"stshowcase1_properties_typeProperties_url": {
						"value": "[parameters('stshowcase1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}